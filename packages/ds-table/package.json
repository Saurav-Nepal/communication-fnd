{
    "name": "@slabs/ds-table",
    "version": "0.0.0",
    "description": "Generic Listing Table",
    "license": "MIT",
    "keywords": [
        "react",
        "ui"
    ],
    "sideEffects": [
        "*.css",
        "*.scss"
    ],
    "entry": "./src/index.ts",
    "main": "./cjs/index.cjs",
    "module": "./esm/index.mjs",
    "types": "./lib/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./lib/index.d.mts",
                "default": "./esm/index.mjs"
            },
            "require": {
                "types": "./lib/index.d.ts",
                "default": "./cjs/index.cjs"
            }
        },
        "./src/styles.scss": "./src/styles.scss"
    },
    "scripts": {
        "build": "esno ../../scripts/build ds-table",
        "dev": "nodemon --exec \"esno ../../scripts/build ds-table\""
    },
    "peerDependencies": {
        "react": "^18.3.1"
    },
    "devDependencies": {
        "@slabs/ds-core": "*",
        "@slabs/ds-hooks": "*",
        "@slabs/ds-utils": "*",
        "@slabs/eslint-config": "*",
        "@types/eslint": "^8.44.7",
        "@types/node": "^20.5.2",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@types/react-truncate": "^2",
        "eslint": "^8.56.0",
        "prop-types": "^15.8.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "typescript": "5.5.4"
    },
    "dependencies": {
        "@radix-ui/react-icons": "^1.3.0",
        "@radix-ui/react-menubar": "^1.1.1",
        "@tanstack/react-query": "^5.51.15",
        "@tanstack/react-table": "^8.19.3",
        "axios": "^1.7.2",
        "class-transformer": "^0.5.1",
        "class-validator-message-formatter": "^0.0.3",
        "date-fns": "^3.6.0",
        "fuse.js": "^7.0.0",
        "next": "^14.2.5",
        "react-content-loader": "^7.0.2",
        "react-querybuilder": "7.6.1",
        "react-truncate": "^2.4.0",
        "state-manager-utility": "^2.0.1",
        "storage-utility": "^3.1.3"
    },
    "nodemonConfig": {
        "ignore": [
            "**/lib/**",
            "**/esm/**",
            "**/cjs/**",
            "**/src/**/*.test.ts"
        ],
        "ext": "*.ts, *.tsx, *.css, *.scss, *.js, *.jsx, *.json",
        "delay": 1500,
        "stdin": false
    }
}
