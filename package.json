{
    "name": "slab-admin",
    "private": true,
    "scripts": {
        "build": "turbo build",
        "build:admin": "turbo build --filter=admin",
        "build:packages": "turbo build --filter='./packages/*'",
        "build:core": "turbo build --filter=@slabs/ds-core",
        "build:utils": "turbo build --filter=@slabs/ds-utils",
        "build:hooks": "turbo build --filter=@slabs/ds-hooks",
        "dev": "turbo dev",
        "dev:admin": "turbo dev --filter=admin",
        "dev:packages": "turbo dev --filter='./packages/*'",
        "dev:core": "turbo dev --filter=@slabs/ds-core",
        "start:admin": "turbo start --filter=admin",
        "lint": "turbo lint",
        "plop": "esno scripts/plop",
        "clean": "esno scripts/clean",
        "storybook": "storybook dev -p 6006",
        "test": "yarn prettier:test && yarn typecheck && yarn eslint && yarn jest",
        "jest": "echo 'Running tests...' && jest",
        "eslint": "echo 'Linting...' && eslint packages --ext .ts,.tsx --cache",
        "typecheck": "echo 'Checking types...' && tsc --noEmit",
        "prettier:test": "prettier --check \"**/*.{ts,tsx,mdx,css}\"",
        "format": "prettier --write \"**/*.{ts,tsx,mdx,css}\""
    },
    "dependencies": {
        "@radix-ui/react-accordion": "^1.1.2",
        "@radix-ui/react-collapsible": "^1.1.0",
        "@radix-ui/react-dismissable-layer": "^1.1.0",
        "@radix-ui/react-label": "^2.1.0",
        "@radix-ui/react-tabs": "^1.1.0",
        "@radix-ui/react-tooltip": "^1.0.7",
        "@types/react-date-range": "^1.4.9",
        "cleave.js": "^1.6.0",
        "clsx": "^2.0.0",
        "date-fns": "^3.6.0",
        "react": "^18.3.1",
        "react-date-range": "^2.0.1",
        "sonner": "^1.5.0",
        "tailwind-merge": "^2.2.0",
        "type-fest": "^4.8.3"
    },
    "devDependencies": {
        "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
        "@rollup/plugin-alias": "^5.1.0",
        "@rollup/plugin-commonjs": "^26.0.1",
        "@rollup/plugin-node-resolve": "^15.2.3",
        "@rollup/plugin-replace": "^5.0.5",
        "@rollup/plugin-typescript": "^11.1.5",
        "@slabs/eslint-config": "*",
        "@storybook/addon-actions": "^7.6.17",
        "@storybook/addon-essentials": "^7.6.17",
        "@storybook/addon-interactions": "^7.6.17",
        "@storybook/addon-styling-webpack": "^0.0.6",
        "@storybook/react": "^7.6.17",
        "@storybook/react-webpack5": "^7.6.17",
        "@testing-library/dom": "^9.3.3",
        "@testing-library/jest-dom": "^6.1.5",
        "@testing-library/react": "^14.1.2",
        "@testing-library/user-event": "^14.5.1",
        "@types/cleave.js": "^1.4.12",
        "@types/eslint": "^8.44.7",
        "@types/fs-extra": "^11.0.4",
        "@types/jest": "^29.5.11",
        "@types/minimatch": "^5.1.2",
        "@types/react-color": "^2",
        "@types/react-dom": "^18",
        "@types/rimraf": "^4.0.5",
        "@types/signale": "^1.4.7",
        "@types/yargs": "^17.0.32",
        "autoprefixer": "^10.4.16",
        "chalk": "^5.3.0",
        "css-loader": "^6.8.1",
        "esbuild": "^0.19.10",
        "esbuild-jest": "^0.5.0",
        "eslint": "^8.56.0",
        "eslint-plugin-log-filenames": "^1.0.6",
        "esno": "^4.0.0",
        "execa": "^8.0.1",
        "fast-glob": "^3.3.2",
        "fs-extra": "^11.2.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "minimatch": "^10.0.1",
        "nodemon": "^3.1.4",
        "postcss": "^8.4.32",
        "postcss-loader": "^7.3.3",
        "prettier": "^3.1.0",
        "react-dom": "^18.3.1",
        "rimraf": "^5.0.5",
        "rollup": "^4.9.1",
        "rollup-plugin-banner2": "^1.2.2",
        "rollup-plugin-esbuild": "^6.1.0",
        "rollup-plugin-lost": "^0.1.1",
        "rollup-plugin-postcss": "^4.0.2",
        "signale": "^1.4.0",
        "storybook": "^7.6.17",
        "storybook-dark-mode": "^3.0.3",
        "style-loader": "^3.3.3",
        "tsc": "^2.0.4",
        "tsconfig-paths-webpack-plugin": "^4.1.0",
        "turbo": "^2.0.12",
        "typescript": "5.5.4",
        "webpack": "^5.89.0",
        "yargs": "^17.7.2"
    },
    "engines": {
        "node": ">=18",
        "npm": "please-use-yarn",
        "yarn": ">=4.4.0"
    },
    "packageManager": "yarn@4.4.0",
    "namespace": "@slabs",
    "workspaces": [
        "apps/*",
        "packages/*"
    ]
}
