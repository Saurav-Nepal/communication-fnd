{
    "private": true,
    "scripts": {
        "build": "turbo run build --filter=@finnoto/web... --filter=@finnoto/pwa...",
        "build:web": "turbo run build --no-update-notifier --filter=@finnoto/web...",
        "build:pwa": "turbo run build --no-update-notifier --filter=@finnoto/pwa...",
        "build:docs": "turbo run build --no-update-notifier --filter=@finnoto/docs...",
        "start": "turbo run start --filter=@finnoto/web... --filter=@finnoto/pwa...",
        "start:web": "turbo run start --filter=@finnoto/web...",
        "start:pwa": "turbo run start --filter=@finnoto/pwa...",
        "dev": "turbo run dev --filter=@finnoto/web... --filter=@finnoto/pwa...",
        "dev:web": "export NODE_OPTIONS=--max-old-space-size=3000 && turbo run dev --no-update-notifier --filter=@finnoto/web...",
        "dev:pwa": "export NODE_OPTIONS=--max-old-space-size=4096 && turbo run dev --no-update-notifier --filter=@finnoto/pwa...",
        "dev:docs": "turbo run dev --no-update-notifier --filter=@finnoto/docs...",
        "clean": "turbo run clean && rm -rf ./node_modules",
        "lint": "turbo run lint --filter=@finnoto/web... --filter=@finnoto/pwa...",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md}\"",
        "docs": "turbo run dev --scope docs",
        "docs:build": "turbo run build --scope docs",
        "save": "bash ./scripts/commit.sh"
    },
    "devDependencies": {
        "@babel/plugin-syntax-jsx": "^7.18.6",
        "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
        "@next/bundle-analyzer": "^13.0.4",
        "@svgr/webpack": "^6.5.1",
        "@turbo/gen": "^1.9.7",
        "@types/jest": "^25.1.4",
        "@types/mime-types": "2.1.4",
        "@types/node": "^18.11.18",
        "@types/react": "^18.2.0",
        "@types/react-beautiful-dnd": "^13.1.3",
        "@types/react-date-range": "^1.4.4",
        "@types/react-dom": "^18.2.0",
        "@types/react-grid-layout": "^1.3.2",
        "@types/react-helmet": "^6.1.6",
        "@types/react-modal": "^3.13.1",
        "@types/react-select": "^5.0.1",
        "@types/react-slick": "^0.23.10",
        "@types/react-truncate": "^2.3.4",
        "@typescript-eslint/eslint-plugin": "^5.48.1",
        "@typescript-eslint/parser": "^5.48.1",
        "autoprefixer": "^10.4.14",
        "eslint": "^7.32.0",
        "eslint-config-custom": "*",
        "eslint-plugin-import": "^2.27.5",
        "next-compose-plugins": "^2.2.1",
        "next-pwa": "^5.6.0",
        "next-sitemap": "^4.1.3",
        "next-transpile-modules": "^10.0.0",
        "next-unused": "^0.0.6",
        "postcss": "8.4.19",
        "prettier": "^2.5.1",
        "sass": "^1.63.2",
        "styled-jsx": "^5.1.1",
        "tsup": "^5.10.1",
        "turbo": "^1.10.16",
        "typescript": "^4.5.2"
    },
    "dependencies": {
        "@hookform/resolvers": "^3.3.1",
        "lottie-react": "^2.4.0",
        "lucide-react": "0.263.1",
        "number-to-words": "^1.2.4",
        "react-cropper": "^2.3.3",
        "react-grid-layout": "^1.3.4",
        "react-hook-form": "~7.46.1",
        "react-image-gallery": "^1.2.12",
        "react-imask": "^6.5.1",
        "react-infinite-scroll-hook": "^4.1.1",
        "react-step-wizard": "^5.3.11",
        "react-terminal-ui": "^1.0.5",
        "type-fest": "^4.14.0"
    },
    "resolutions": {
        "@types/mime": "3.0.4"
    },
    "packageManager": "yarn@1.22.19",
    "namespace": "@finnoto",
    "workspaces": [
        "apps/*",
        "packages/*"
    ]
}
